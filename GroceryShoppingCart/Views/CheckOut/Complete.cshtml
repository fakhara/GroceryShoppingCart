@model int
@{
    ViewBag.Title = "Checkout Complete";
}
<div class="row" style="background-color:gainsboro">
<h2>Congratulations!!! Your Order is Placed Successfully</h2>
<p>Thanks for your order! Your order number is: @Model</p>
<p>
    How about shopping for some more Products in our
    @Html.ActionLink("Store", "Index", "Home")
</p>
<p>
    Check You Order Details:
    @Html.ActionLink("Order Details", "Details", "Orders", new { id = Model }, null)
</p>
</div>