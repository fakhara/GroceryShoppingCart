@{
    ViewBag.Title = "Contact";
}
<br/><br/>
<style>
    /*Alway set the map height explicity todefine the size of the div element that contain the map.*/
    #map {
        height: 400px;
        width: 100%;
    }
</style>
<div class="row">
    <div id="map"></div>
    <script>
        var map;
        function initMap() {
            map = new google.maps.Map(document.getElementById('map'), {
                center: { lat: 56.879004, lng: 14.805852 },
                zoom: 16
            });
            var marker = new google.maps.Marker({
                position: { lat: 56.879004, lng: 14.805852 },
                    map: map
            });
        }
    </script>
    <script src="https://maps.googleapis.com/maps/api/js?key=AIzaSyBu-916DdpKAjTmJNIgngS6HL_kDIKU0aU&callback=initMap" async defer></script>
</div>
<h2>@ViewBag.Title.</h2>
<h3>@ViewBag.Message</h3>

<address>
    One Microsoft Way<br />
    Redmond, WA 98052-6399<br />
    <abbr title="Phone">P:</abbr>
    425.555.0100
</address>

<address>
    <strong>Support:</strong>   <a href="mailto:Support@example.com">Support@example.com</a><br />
    <strong>Marketing:</strong> <a href="mailto:Marketing@example.com">Marketing@example.com</a>
</address>