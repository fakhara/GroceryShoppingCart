@{
    ViewBag.Title = "Welcome";
}
<div class="container">
    <div id="myCarousel" class="carousel slider" data-ride="carousel">
        <!--Indicators-->
        <ul class="carousel-indicators">
            <li data-target="#myCarousel" data-slide="0" class="active"></li>
            <li data-target="#myCarousel" data-slide="1"></li>
            <li data-target="#myCarousel" data-slide="2"></li>
            <li data-target="#myCarousel" data-slide="3"></li>
            <li data-target="#myCarousel" data-slide="4"></li>
            <li data-target="#myCarousel" data-slide="5"></li>
            <li data-target="#myCarousel" data-slide="6"></li>
            <li data-target="#myCarousel" data-slide="7"></li>
            <li data-target="#myCarousel" data-slide="8"></li>
            <li data-target="#myCarousel" data-slide="9"></li>
            <li data-target="#myCarousel" data-slide="10"></li>
        </ul>
        <!--Wrapper for slide-->
        <div class="carousel-inner">
            <div class="item active">
                <img src="~/Content/image/foodGrocery.png" alt="Food & Grocery" style="width:100%" />
            </div>
            <div class="item">
                <img src="~/Content/image/Beverage.png" alt="Beverage" style="width:100%" />
            </div>
            <div class="item">
                <img src="~/Content/image/bread.png" alt="Bread & Bakrey" style="width:100%" />
            </div>
            <div class="item">
                <img src="~/Content/image/dairyproducts.png" alt="Dairy Products" style="width:100%" />
            </div>
            <div class="item">
                <img src="~/Content/image/dryfruit.png" alt="Dry Products" style="width:100%" />
            </div>
            <div class="item">
                <img src="~/Content/image/frozenfood.png" alt="Frozen Products" style="width:100%" />
            </div>
            <div class="item">
                <img src="~/Content/image/clearner.png" alt="Cleaner" style="width:100%" />
            </div>
            <div class="item">
                <img src="~/Content/image/personalCare.png" alt="Personal Care" style="width:100%" />
            </div>
            <div class="item">
                <img src="~/Content/image/fruitvegetables.png" alt="Fruit & Vegetables" style="width:100%" />
            </div>
            <div class="item">
                <img src="~/Content/image/babycareProducts.png" alt="Baby Care" style="width:100%" />
            </div>
            <div class="item">
                <img src="~/Content/image/seasoningsspices.png" alt="Seasoning & Spice" style="width:100%" />
            </div>
        </div>
        <!--Left and right controls-->
        <a class="left carousel-control" href="#myCarousel" data-slide="prev">
            <span class="glyphicon glyphicon-chevron-left"></span>
            <span class="sr-only">Previous</span>
        </a>
        <a class="right carousel-control" href="#myCarousel" data-slide="next">
            <span class="glyphicon glyphicon-chevron-right"></span>
            <span class="sr-only">Next</span>
        </a>
    </div>
</div>
